{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "quotes": ["off", "single"],
  "trailingComma": "es5",
  "printWidth": 80,
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "auto",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "jsxSingleQuote": false
}
